<html lang='en'>

<%- include('./partials/head.ejs') %>

    <body>

        <%- include('./partials/nav.ejs') %>
            <div class='title'>Feedback</div>
            <div class='filler content'>
                <h2>Insert Filler</h2>

                <% if (testInput.length> 0) { %>
                    <% testInput.forEach(testInput=> { %>
                        <h3 class='title'>
                            <%= testInput.title %>
                        </h3>
                        <p class='snippet'>
                            <%= testInput.snippet %>
                        </p>
                        <%}) %>
                            <% } else { %>
                                <p>Nothing to display</p>
                                <% } %>
            </div>

            <%- include('./partials/footer.ejs') %>
    </body>

</html>